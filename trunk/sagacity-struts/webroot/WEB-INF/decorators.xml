<decorators defaultdir="/common/layouts">
	<excludes>
		<pattern>/demos/*</pattern>
		<pattern>/dwr/*</pattern>
		<pattern>/dwr/interface/*</pattern>
		<pattern>/common/*</pattern>
		<pattern>/common/submit_again.jsp</pattern>
		<pattern>/common/error.jsp</pattern>
		<pattern>/inventory/selectstock*</pattern>
		<pattern>/resources/*</pattern>
		<pattern>/**/print*</pattern>
		<pattern>/login.jsp*</pattern>
		<pattern>/business/myMailAction.do?method=download*</pattern>
		<pattern>/logout.jsp*</pattern>
		<pattern>/main.jsp*</pattern>
		<pattern>/servlet/download/*</pattern>
		<pattern>/produce/fetchMaterials.do?method=print*</pattern>
		<pattern>/inventory/maintainOutbounds.do?method=print*</pattern>
		<pattern>/purchase/maintainOrder.do?method=print*</pattern>
		<pattern>/purchase/maintainOrder.do?method=export*</pattern>
		<pattern>/quality/chkReportAction.do?method=print*</pattern>
		<pattern>
			/quality/maintainCheckReportPrints.do?method=print*
		</pattern>
	</excludes>
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
